"*":
  "atom-alignment": {}
  "autocomplete-elixir":
    elixirPath: "/usr/local/bin/elixir"
    elixrPath: "/usr/local/bin/elixir"
  "autocomplete-plus":
    confirmCompletion: "enter"
  "base16-syntax":
    scheme: "Ocean"
  core:
    audioBeep: false
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "language-elixir"
      "linter-rubocop"
      "linter-elixirc"
      "autocomplete-elixir"
      "linter-htmlhint"
      "linter-jshint"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      "log"
      "repositories"
      "tmp"
      "vendor"
      ".bundle"
      "coverage"
      "*.class"
      ".DS_Store"
    ]
    projectHome: "/Users/steveagalloco/Projects"
    themes: [
      "unity-ui"
      "base16-syntax"
    ]
  editor:
    fontFamily: "Menlo"
    fontSize: 12
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
  "git-blame": {}
  "go-config": {}
  "go-plus": {}
  linter:
    subtleLinterErrors: [
      "ruby"
    ]
  "linter-elixirc":
    elixircExecutablePath: "/usr/local/bin/elixirc"
  "linter-eslint":
    fixOnSave: true
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
  "linter-rubocop": {}
  "linter-rust":
    cargoPath: "/usr/local/bin/cargo"
    rustcPath: "/usr/local/bin/rustc"
  minimap: {}
  "release-notes":
    viewedVersion: "0.89.0"
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  "tree-view":
    hideVcsIgnoredFiles: true
  "unity-ui":
    colorStatusIndicatorsInTreeView: true
  welcome:
    showOnStartup: false
  "xml-formatter":
    numberCharIndent: 4
