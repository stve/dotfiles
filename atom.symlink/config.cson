    fontFamily: "Menlo"
    fontSize: 12
    showInvisibles: true
    showIndentGuide: true
    invisibles: {}
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    themes: [
      "unity-dark-ui"
      "monokai"
    ]
    projectHome: "/Users/steveagalloco/Projects"
